USE CLOUDWICK;





INSERT INTO TABLE EMPDEPT
SELECT EMP.ID as EMPID,EMP.NAME as EMPNAME,DEPT.NAME AS DEPTNAME FROM EMPLOYEES EMP
JOIN DEPARTMENTS DEPT ON (EMP.DEPTID=DEPT.DEPTID) WHERE EMP.ID IN (1007,1008);INSERT INTO TABLE PARTDEPT
PARTITION(DEPTNAME='BI')
SELECT EMPID,EMPNAME FROM EMPDEPT WHERE EMPID IN (1007,1008) AND DEPTNAME='BI';
